class Solution {
    public String toLowerCase(String s) {
        String a="";
       for(int i=0;i<s.length();i++){
           if(s.charAt(i)-'A'>=0&&s.charAt(i)-'A'<=25){
              a=a+(char)(s.charAt(i)+32);
           }
           else{
               a=a+s.charAt(i);
           }
       }
        return a;
    
    }
}
